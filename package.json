{
  "name": "portal-colaborador-cypress",
  "version": "1.0.0",
  "description": "Testes automatizados do Portal do Colaborador (Cypress + Mochawesome)",
  "devDependencies": {
    "cypress": "^15.6.0",
    "cypress-downloadfile": "^1.2.4",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-terminal-report": "^7.3.3",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.3.2",
    "npm-run-all": "^4.1.5",
    "pdfjs-dist": "^5.4.394"
  },
  "scripts": {
    "test": "cypress run",
    "report": "npm run test && npm run report:generate && start cypress\\reports\\html\\mochawesome.html",
